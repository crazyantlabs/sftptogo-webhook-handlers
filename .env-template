# SFTP Configuration
SFTP_HOST=example.sftptogo.com
SFTP_PORT=22
SFTP_USER=username
SFTP_PASSWORD=your-password

# Webhook Verification Secret
WEBHOOK_SECRET=your-shared-secret

# PGP Private Key (Base64 Encoded or Inline if small)
PGP_PRIVATE_KEY="-----BEGIN PGP PRIVATE KEY-----\n...\n-----END PGP PRIVATE KEY-----"
PGP_PRIVATE_KEY_PASSPHRASE=your-passphrase

# Upload Target Path
UPLOAD_PATH=decrypted
UPLOAD_PATH_IS_RELATIVE=true

# Delete Original After Upload
DELETE_AFTER_UPLOAD=true